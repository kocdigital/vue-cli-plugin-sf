{
  "compilerOptions": {
    "target": "es2015",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "module": "node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "@vue/cli-service",
    ]
  },
  "exclude": ["dist"],
}
